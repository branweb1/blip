{
  "name": "blip",
  "description": "Speed reader extension",
  "version": "0.0.1",
  "manifest_version": 3,
  "permissions": [ "activeTab", "storage" ],
  "action": {
    "default_icon": "icons/heartbeat.png",
    "default_popup": "index.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
