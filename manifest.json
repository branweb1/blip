{
  "name": "blip",
  "description": "Speed reader extension",
  "version": "0.0.2",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [ "activeTab", "storage" ],
  "action": {
    "default_icon": "heartbeat.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
